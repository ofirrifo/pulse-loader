{
  "name": "@rifo/pulse-loader",
  "version": "0.0.3",
  "scripts": {
    "ng": "ng",
    "all:build": "npm run example:build && npm run lib:build",
    "example:start": "ng serve --open",
    "example:build": "ng build pulse-loader-example --prod --aot --buildOptimizer",
    "example:publish": "npm run example:build && ngh --dir=./dist/pulse-loader-example --no-silent",
    "example:lint": "ng lint pulse-loader-example --fix",
    "lib:build": "ng build pulse-loader",
    "lib:lint": "ng lint pulse-loader --fix",
    "lib:publish2npm": "npm run lib:build && ./dist/pulse-loader npm publish"
  },
  "description": "angular pulse loader",
  "keywords": [
    "pulse",
    "loader",
    "pulse loader",
    "angular pulse loader"
  ],
  "author": "Ofir Fridman",
  "repository": {
    "type": "git",
    "url": "https://github.com/ofirrifo/pulse-loader.git"
  },
  "homepage": "https://github.com/ofirrifo/pulse-loader",
  "engines": {
    "node": ">=10.9.0 <13.0.0",
    "yarn": ">=1.9.0 <2.0.0"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.3",
    "@angular/common": "~8.2.3",
    "@angular/compiler": "~8.2.3",
    "@angular/core": "~8.2.3",
    "@angular/forms": "~8.2.3",
    "@angular/platform-browser": "~8.2.3",
    "@angular/platform-browser-dynamic": "~8.2.3",
    "@angular/router": "~8.2.3",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.0",
    "@angular-devkit/build-ng-packagr": "~0.803.0",
    "@angular/cli": "~8.3.0",
    "@angular/compiler-cli": "~8.2.3",
    "@angular/language-service": "~8.2.3",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.7.2",
    "codelyzer": "^5.0.0",
    "husky": "^3.0.4",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lint-staged": "^9.2.3",
    "ng-packagr": "^5.1.0",
    "prettier": "1.18.2",
    "protractor": "~5.4.0",
    "ts-node": "~8.3.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "projects/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  }
}
