{
  "name": "pulse-loader",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "all:build": "npm run example:build && npm run lib:build",
    "example:start": "ng serve --open",
    "example:build": "ng build pulse-loader-example --prod --aot --buildOptimizer",
    "example:publish": "npm run example:build && ngh --dir=./dist/pulse-loader-example --no-silent",
    "example:lint": "ng lint pulse-loader-example --fix",
    "lib:build": "ng build pulse-loader",
    "lib:lint": "ng lint pulse-loader --fix",
    "lib:publish": "./dist/pulse-loader npm publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0-next.5",
    "@angular/common": "~9.0.0-next.5",
    "@angular/compiler": "~9.0.0-next.5",
    "@angular/core": "~9.0.0-next.5",
    "@angular/forms": "~9.0.0-next.5",
    "@angular/platform-browser": "~9.0.0-next.5",
    "@angular/platform-browser-dynamic": "~9.0.0-next.5",
    "@angular/router": "~9.0.0-next.5",
    "rxjs": "~6.5.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.10.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.0-next.3",
    "@angular-devkit/build-ng-packagr": "~0.900.0-next.3",
    "@angular/cli": "~9.0.0-next.3",
    "@angular/compiler-cli": "~9.0.0-next.5",
    "@angular/language-service": "~9.0.0-next.5",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.7.2",
    "codelyzer": "^5.0.0",
    "husky": "^3.0.4",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lint-staged": "^9.2.3",
    "ng-packagr": "^5.4.0",
    "prettier": "1.18.2",
    "protractor": "~5.4.0",
    "ts-node": "~8.3.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "projects/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  }
}
